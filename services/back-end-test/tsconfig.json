{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": ["es2021", "dom"],
		"allowJs": false,
		"outDir": "build",
		"strict": false,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strictNullChecks": false,
		"paths": {
			"@src/*": ["src/*"]
		},
		"baseUrl": "./",
		"downlevelIteration": true
	}
}
